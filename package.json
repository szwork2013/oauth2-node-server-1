{
  "name": "oauth2-node-server",
  "version": "0.0.1",
  "description": "Small example that uses oauth2 and mongoose",
  "main": "dist/index.js",
  "scripts": {
    "start": "gulp serve",
    "build": "gulp",
    "lint": "gulp lint",
    "test": "gulp mocha",
    "commit": "git-cz",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jermorin/oauth2-node-server.git"
  },
  "keywords": [
    "oauth2",
    "server"
  ],
  "dependencies": {
    "bcrypt": "^0.8.6",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "cookie-session": "^2.0.0-alpha.1",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "express-validation": "^0.6.0",
    "express-winston": "^1.3.0",
    "http-status": "^0.2.2",
    "jade": "*",
    "joi": "^8.0.5",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.11.1",
    "method-override": "^2.3.5",
    "mongoose": "^4.4.13",
    "morgan": "1.7.0",
    "node-oauth2-server": "^2.4.0",
    "request": "^2.72.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.7.7",
    "babel-core": "6.7.7",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-2": "6.5.0",
    "chai": "^3.5.0",
    "commitizen": "2.8.0",
    "coveralls": "^2.11.9",
    "cz-conventional-changelog": "1.1.6",
    "del": "^2.2.0",
    "ghooks": "1.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-env": "^0.4.0",
    "gulp-istanbul": "^0.10.4",
    "gulp-load-plugins": "^1.2.2",
    "gulp-mocha": "^2.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-plumber": "^1.1.0",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-util": "^3.0.7",
    "isparta": "4.0.0",
    "mocha": "^2.4.5",
    "run-sequence": "^1.1.5",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.1.0",
    "validate-commit-msg": "2.6.1"
  },
  "author": "Jermorin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jermorin/oauth2-node-server/issues"
  },
  "homepage": "https://github.com/Jermorin/oauth2-node-server#readme",
  "config": {
    "ghooks": {
      "pre-commit": "",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
